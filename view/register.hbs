<html>
  <head>
    {{> head}}
    <link rel="stylesheet" href="css/register.scss">
	</head>
	<body>
	{{> header}}
    <div id="Registration">
      <h1>Register for Alive</h1>
      <h2>Sign Up for more!</h2>
      <form class="form-inline" id="name">
        <div class="form-group">
          <label for="exampleInputName2">First Name</label>
          <input type="text" class="form-control" id="exampleInputName2" placeholder="First Name">
        </div>
        <div class="form-group">
          <label for="exampleInputName2">Last Name</label>
          <input type="text" class="form-control" id="exampleInputName2" placeholder="Last Name">
        </div>
      </form>
      <div class="form-group row">
        <label for="example-email-input" class="col-xs-2 col-form-label">Email</label>
        <div class="col-xs-10">
          <input class="form-control" type="email" value="bootstrap@example.com" id="example-email-input">
        </div>
      </div>
      <div class="form-group row">
        <label for="example-password-input" class="col-xs-2 col-form-label">Password</label>
        <div class="col-xs-10">
          <input class="form-control" type="password" value="hunter2" id="example-password-input">
        </div>
      </div>
      <div class="form-group row">
        <label for="example-tel-input" class="col-xs-2 col-form-label">Telephone</label>
        <div class="col-xs-10">
          <input class="form-control" type="tel" value="1-(555)-555-5555" id="example-tel-input">
        </div>
      </div>
      <div class="form-group row">
        <label for="example-date-input" class="col-xs-2 col-form-label">Birthday</label>
        <div class="col-xs-10">
          <input class="form-control" type="date" value="2011-08-19" id="example-date-input">
        </div>
      </div>
    <button type="button" class="btn btn-outline-primary" id="submit">Submit</button>
    </div>
  {{> footer }}
  </body>
</html>
